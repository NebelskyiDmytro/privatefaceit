  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Backend debug",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
          "start:debug"
      ],
      "console": "internalConsole",
      "outputCapture": "std",
      "env": {
          "ENV": "development",
          "NODE_ENV": "development",
      },
      "preLaunchTask": "yarn build",
      "sourceMaps": true,
      "port": 9229,
      "outFiles": ["${workspaceFolder}/apps/backend/dist/**/*"]
  },
  ]
}